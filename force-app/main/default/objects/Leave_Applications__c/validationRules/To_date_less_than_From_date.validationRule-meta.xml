<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>To_date_less_than_From_date</fullName>
    <active>true</active>
    <errorConditionFormula>To__c &lt;  From__c</errorConditionFormula>
    <errorDisplayField>To__c</errorDisplayField>
    <errorMessage>To date cannot be less than From date!!</errorMessage>
</ValidationRule>
