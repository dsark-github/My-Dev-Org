<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Allocated_Date_After_Released_Date</fullName>
    <active>true</active>
    <errorConditionFormula>Allocated_Date__c &gt; Released_Date__c</errorConditionFormula>
    <errorDisplayField>Released_Date__c</errorDisplayField>
    <errorMessage>Released Date must be after Allocated Date!!</errorMessage>
</ValidationRule>
