<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Joining_Date_Before_Creation_Date</fullName>
    <active>true</active>
    <errorConditionFormula>Date_of_Joining__c &lt; DATEVALUE(CreatedDate)</errorConditionFormula>
    <errorDisplayField>Date_of_Joining__c</errorDisplayField>
    <errorMessage>Joining Date cannot be before creation date!!</errorMessage>
</ValidationRule>
